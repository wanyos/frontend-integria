{
  "name": "integria-dashboard",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev:open": "vite --open --mode development",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "cypress open",
    "test:electron": "cross-env NODE_NO_WARNINGS=1 cypress run",
    "test:all": "cross-env NODE_NO_WARNINGS=1 cypress run --headless --browser chrome",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{js,vue,json,css,scss}\" \"cypress/**/*.{js,ts,jsx,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{js,vue,json,css,scss}\" \"cypress/**/*.{js,ts,jsx,tsx}\""
  },
  "dependencies": {
    "apexcharts": "^4.7.0",
    "bcrypt": "^5.1.1",
    "dayjs": "^1.11.13",
    "echarts": "^5.6.0",
    "exceljs": "^4.4.0",
    "execSync": "^1.0.2",
    "json-2-csv": "^5.5.9",
    "mysql2": "^3.14.0",
    "oh-vue-icons": "^1.0.0-rc3",
    "pinia": "^3.0.2",
    "radix-vue": "^1.9.17",
    "vue": "^3.5.13",
    "vue-cookies": "^1.8.6",
    "vue-echarts": "^7.0.3",
    "vue-loading-overlay": "^6.0.6",
    "vue-router": "^4.5.1",
    "vue3-apexcharts": "^1.8.0",
    "vue3-toastify": "^0.2.8",
    "vuejs3-datepicker": "^1.1.3"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitest/eslint-plugin": "1.1.43",
    "@vue/test-utils": "^2.4.6",
    "cross-env": "^7.0.3",
    "cypress": "^14.3.2",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-cypress": "^4.3.0",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-vue": "^10.0.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.5.3",
    "vite": "^6.3.3",
    "vite-plugin-vue-devtools": "^7.7.5",
    "vitest": "^3.1.2"
  }
}
